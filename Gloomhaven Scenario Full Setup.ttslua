--Table Deep Copy
function deepCopy(orig)
	local orig_type = type(orig)
	local copy
	if orig_type == 'table' then
		copy = {}
		for orig_key, orig_value in next, orig, nil do
			copy[deepCopy(orig_key)] = deepCopy(orig_value)
		end
		setmetatable(copy, deepCopy(getmetatable(orig)))
	else -- number, string, boolean, etc
		copy = orig
	end
	return copy
end




--Get Treasure Card
function getTreasureCard(params)
	Unlockable = getObjectFromGUID(Unlockable_GUID)

	--Draw Treasure Deck out of Unlockable Chest
	for _,obj in ipairs(Unlockable.getObjects()) do
		if obj.name == String_TreasureIndexDeck then
			local pos = Unlockable.getPosition()
			pos.y = pos.y - 10
			Unlockable.takeObject({
				guid			= obj.guid
				,position		= pos
				,smooth			= false
				,callback		= "spawnCallback"
				,callback_owner	= self
				,params			= {
					lock	= true
				}
			})
			break
		end
	end

	function registerDeck_DrawCard()
		wait(Delay_BagRegister)

		local AllObjects = getAllObjects()
		local CardName
		local TreasureDeck_GUID
		local TreasureDeck

		for _,obj in ipairs(AllObjects) do
			if obj.getName() == String_TreasureIndexDeck then
				TreasureDeck_GUID = obj.getGUID()
			end
		end

		TreasureDeck = getObjectFromGUID(TreasureDeck_GUID)

		CardName = "Treasure #"
		if params.numTreasure < 10 then CardName = CardName .. "0" end
		CardName = CardName .. params.numTreasure

		for _,card in ipairs(TreasureDeck.getObjects()) do
			if card.nickname == CardName then
				TreasureDeck.takeObject({
					guid			= card.guid
					,position		= {x=-22.312502, y=1.771799, z=3.788862}
					,rotation		= {x=-0.000252, y=180.000000, z=-0.000764}
					,smooth			= false
					,callback		= "spawnCallback"
					,callback_owner	= self
					,params			= {
						lock	= false
					}
				})
				break
			end
		end

		Unlockable.putObject(TreasureDeck)
		getObjectFromGUID(params.treasureTile_GUID).destruct()

		return 1
	end
	startLuaCoroutine(self, "registerDeck_DrawCard")
end




--Generic Spawn Callback Function
function spawnCallback(obj,params)
	local ScriptConstruct = ""

	if params.script ~= nil then
		ScriptConstruct = params.script
	end

	if params.buttonTheme ~= nil then
		local ButtonLabel

		if params.buttonLabel ~= nil then
			ButtonLabel = params.buttonLabel
		elseif params.buttonTheme == "Door" then
			ButtonLabel = "Open"
		elseif params.buttonTheme == "Start" then
			ButtonLabel = "Start"
		end

		ScriptConstruct = ScriptConstruct
..		"\nfunction onLoad()"
..		"\n	local ButtonTable = {"
..		"\n		label			= '" .. ButtonLabel .. "'"
..		"\n		,click_function	= 'selfFunction'"
..		"\n		,function_owner	= self"
..		"\n		,position		= {x=0,y=0,z=0}"
..		"\n		,rotation		= {x=0,y=180-self.getRotation().y,z=0}"

		if params.buttonTheme == "Door" then
			ScriptConstruct = ScriptConstruct
..		"\n		,color			= {0.408,0.525,0.910}"
..		"\n		,height			= 250"
..		"\n		,width			= 500"
..		"\n		,font_size		= 200"
		elseif params.buttonTheme == "Start" then
			ScriptConstruct = ScriptConstruct
..		"\n		,color			= {0.612,0.678,0.741}"
..		"\n		,height			= 250"
..		"\n		,width			= 500"
