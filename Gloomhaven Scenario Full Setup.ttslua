		spawnObject(i).setCustomObject(i)
		position.x = position.x + 10.5
	end
end




--Take Back from Infinite Bag for Use
function takeBagFromInfinite(InfiniteBag,FutureName)
	local pos = InfiniteBag.getPosition()
	pos.y = pos.y - 10
	local BagGUID = InfiniteBag.takeObject({
		position		= pos
		,smooth			= false
		,callback		= "spawnCallback"
		,callback_owner	= self
		,params			= {
			lock	= true
			,name	= FutureName
		}
	}).getGUID()
	return BagGUID
end




--Wait Time
function wait(time)
	local start = os.time()
	repeat coroutine.yield(0) until os.time() > start + time
end




------------
--Create Map
------------
function createMap()
	if IsBusy == 1 then
		broadcastToAll(String_Busy, {1,1,1})
		return
	end
	IsBusy = 1




	Tiles_MonsterUnspawned = {}
	local ScenarioInformation = deepCopy(ScenarioInformation_All[NumScenario])
	local tMonsters = {}
	local FullMonsterList = {}




	--------------
	--Move Spawner
	--------------
	-- self.setPositionSmooth(
	--	{x=-12.687500, y=1.797194, z=29.553116}
	--	,false
	--	,true
	-- )




	----------------------
	--Spawn Scenario Pages
	----------------------
	spawnScenarioPages(ScenarioInformation.scenarioPages)




	---------------------------------------
	--Spawn Map, Overlay, Start, Door Tiles
	---------------------------------------
	if TypeMap == Constant_TypeMap_Hidden then
		local tMapTiles = {}
		for _,t in ipairs(ScenarioInformation.mapTiles) do
			table.insert(tMapTiles, t)
			if t.startTiles ~= nil then break end
		end

		local tDoorTiles = ScenarioInformation.doorTiles
		if ScenarioInformation.doorTiles ~= nil then

			for iDoorTiles=#tDoorTiles,1,-1 do
				local tType = tDoorTiles[iDoorTiles]
				for iTiles=#tType.tiles,1,-1 do
					local tTiles = tType.tiles[iTiles]
					local ToDelete = 1

					for _,tAdjacent in ipairs(tTiles.adjacentTiles) do
						for _,tMapTiles in ipairs(tMapTiles) do
							if tAdjacent == tMapTiles.tile then
								ToDelete = 0
								break
							end
						end
					end

					if ToDelete == 1 then
						table.remove(tType.tiles, iTiles)
					end
				end
			end
		end

		spawnRoomTiles(tMapTiles,tDoorTiles,nil)
	else
		spawnRoomTiles(ScenarioInformation.mapTiles,ScenarioInformation.doorTiles,nil)
	end




	----------
	--Monsters
	----------
	for _,tMapTiles in ipairs(ScenarioInformation.mapTiles) do
		table.insert(tMonsters, tMapTiles)
		if tMapTiles.startTiles ~= nil then break end
	end
	spawnMonsters(tMonsters,ScenarioInformation.gridType)




	--------------------
	--Monster Stat Cards
	--------------------
	if TypeMap == Constant_TypeMap_Full then
		if ScenarioInformation.monsterList ~= nil then
			for _,tMonsterList in ipairs(ScenarioInformation.monsterList) do
				local params = {}

				params.monsterName = tMonsterList

				if TypeMonster == Constant_TypeMonster_Standee then
					params.monsterType = "Standee"
				elseif TypeMonster == Constant_TypeMonster_Hex then
					if GridType == Constant_Grid_Horizontal then
						params.monsterType = "Horizontal"
					elseif GridType == Constant_Grid_Vertical then
						params.monsterType = "Vertical"
					end
				end


				params.monsterPlacement = false

				--Call Spawn Monster
				getObjectFromGUID(Scripts_GUID).call("spawnMonster", params)
			end
		end
	end



	----------
	--Clean Up
	----------
	function cleanUpParameters()
		wait(Delay_Delete)
		IsBusy = 0
		return 1
	end
	startLuaCoroutine(self, "cleanUpParameters")
end




