		elseif params.buttonTheme == "Treasure" then
			ScriptConstruct = ScriptConstruct
..		"\n		,color			= {0.941,0.694,0.027}"
..		"\n		,height			= 250"
..		"\n		,width			= 280"
..		"\n		,font_size		= 200"
		end

		ScriptConstruct = ScriptConstruct
..		"\n	}"

		if params.buttonTheme == "Door" then
			ScriptConstruct = ScriptConstruct
..		"\n	if"
..		"\n		self.getName() == 'Stone Door Horizontal'"
..		"\n		or self.getName() == 'Stone Door Vertical'"
..		"\n		or self.getName() == 'Wooden Door Horizontal'"
..		"\n		or self.getName() == 'Wooden Door Vertical'"
..		"\n	then"
..		"\n		ButtonTable.position.y = -0.13"
..		"\n		ButtonTable.rotation.z = 180"
..		"\n	end"
..		"\n"
..		"\n	if ButtonTable.label == 'Teleport' then"
..		"\n		ButtonTable.width = 700"
..		"\n	end"
		end


		ScriptConstruct = ScriptConstruct
..		"\n	self.createButton(ButtonTable)"
..		"\nend"
..		"\n"
..		"\nfunction selfFunction()"

		if params.scriptFunction ~= nil then
			ScriptConstruct = ScriptConstruct
..		"\n	" .. params.scriptFunction
		end

		if params.buttonTheme == "Door" then
			ScriptConstruct = ScriptConstruct
..		"\n	local params = {"
..		"\n			adjacentTiles	= AdjacentTiles"
..		"\n			,doorTile_GUID	= self.getGUID()"
..		"\n			,numScenario	= NumScenario"
..		"\n	}"
..		"\n"
..		"\n	getObjectFromGUID('" .. self.getGUID() .. "').call('revealRoom',params)"
		elseif params.buttonTheme == "Start" then
			ScriptConstruct = ScriptConstruct
..		"\n	local AllObjects = getAllObjects()"
..		"\n"
..		"\n	for _,obj in ipairs(AllObjects) do"
..		"\n		if obj.getName() == 'Start Area' then"
..		"\n			obj.destruct()"
..		"\n		end"
..		"\n	end"
		elseif
			params.buttonTheme == "Treasure"
			and ButtonLabel ~= "G"
		then
			ScriptConstruct = ScriptConstruct
..		"\n	local params = {"
..		"\n		numTreasure			= NumTreasure"
..		"\n		,treasureTile_GUID	= self.getGUID()"
..		"\n}"
..		"\n	getObjectFromGUID('" .. self.getGUID() .. "').call('getTreasureCard',params)"
		end

		ScriptConstruct = ScriptConstruct
..		"\nend"
	end

	obj.setLuaScript(ScriptConstruct)

	if params.description ~= nil then
		obj.setDescription(params.description)
	elseif params.buttonTheme == "Start" then
		obj.setDescription("Click to remove Start Area")
	end

	if params.lock == true then
		obj.lock()
	end

	if params.name ~= nil then
		obj.setName(params.name)
	elseif params.buttonTheme == "Start" then
		obj.setName("Start Area")
	end

	if params.colourTint ~= nil then
		obj.setColorTint(params.colourTint)
	end
end




--Spawn Monsters
function spawnMonsters(params_MapTiles,GridType,params_AdjacentTiles)
	function selfFunction()
		wait(Delay_Spawning)

		for _,tMapTiles in ipairs(params_MapTiles) do
			for iUnspawned,tUnspawned in ipairs(Tiles_MonsterUnspawned) do
				if tUnspawned == tMapTiles.tile then
					table.remove(Tiles_MonsterUnspawned, iUnspawned)

					if tMapTiles.monsters ~= nil then
						for _,tMonsters in ipairs(tMapTiles.monsters) do
							if tMonsters.tiles[1].order ~= nil then
								for _,tAdjacent in ipairs(params_AdjacentTiles) do
									if tMonsters.tiles[1].order[tAdjacent] ~= nil then
										table.sort(
											tMonsters.tiles
											,function(a,b)
												return a.order[tAdjacent] < b.order[tAdjacent]
											end
											)
										break
									end
								end
							end

							for _,tIndividual in ipairs(tMonsters.tiles) do
								if tIndividual.numCharacters[NumCharacters] ~= nil then
									local params = {}

									--Set Parameters
									params.monsterName = tMonsters.name

									if TypeMonster == Constant_TypeMonster_Standee then
										params.monsterType = "Standee"
									elseif TypeMonster == Constant_TypeMonster_Hex then
										if GridType == Constant_Grid_Horizontal then
											params.monsterType = "Horizontal"
										elseif GridType == Constant_Grid_Vertical then
											params.monsterType = "Vertical"
										end
									end

									if tIndividual.numCharacters[NumCharacters] == Constant_LevelMonster_Normal then
										params.monsterDifficulty = "normal"
									elseif tIndividual.numCharacters[NumCharacters] == Constant_LevelMonster_Elite then
										params.monsterDifficulty = "elite"
									end

									params.monsterPosition = tIndividual.position

									--Call Spawn Monster
									getObjectFromGUID(Scripts_GUID).call("spawnMonster", params)
								end
							end
						end
					end

					break
				end
			end
		end

		return 1
	end
	startLuaCoroutine(self, "selfFunction")
end




--Spawn Room All Tile Types
function spawnRoomTiles(MapTiles,DoorTiles)
	Timer.destroy("BagCleanUp" .. self.getGUID())




	-----------------------------
