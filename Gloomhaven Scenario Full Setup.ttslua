--Reveal Rooms after initial setup from door
--------------------------------------------
function revealRoom(params)
	if IsBusy == 1 then
		broadcastToAll(String_Busy, {1,1,1})
		return
	end
	IsBusy = 1

	local AllObjects = getAllObjects()
	local ScenarioInformation = deepCopy(ScenarioInformation_All[params.numScenario])
	local params_DoorTiles = {}
	local params_MapTiles = {}
	local params_Monsters = {}




	-----------
	--Flip Door
	-----------
	function flipDoorTile()
		wait(Delay_Spawning+1)
		local position = getObjectFromGUID(params.doorTile_GUID).getPosition()
		position.y = position.y + 0.2
		getObjectFromGUID(params.doorTile_GUID).setPosition(position)
		getObjectFromGUID(params.doorTile_GUID).unlock()
		getObjectFromGUID(params.doorTile_GUID).flip()
		wait(3)
		getObjectFromGUID(params.doorTile_GUID).lock()

		return 1
	end
	startLuaCoroutine(self, "flipDoorTile")




	------------------------------------------
	--Room Tiles into parameter if not present
	------------------------------------------
	for _,tAdjacent in ipairs(params.adjacentTiles) do
		local IsSpawned = 0
		for _,obj in ipairs(AllObjects) do
			if tAdjacent == obj.getName() then
				IsSpawned = 1
				break
			end
		end

		if IsSpawned == 0 then
			for _,tMapTiles in ipairs(ScenarioInformation.mapTiles) do
				if tMapTiles.tile == tAdjacent then
					table.insert(params_MapTiles, tMapTiles)
				end
			end
		end
	end




	------------------------------------------
	--Door Tiles into Parameter if not present
	------------------------------------------
	params_DoorTiles = ScenarioInformation.doorTiles

	for iDoorTiles=#params_DoorTiles,1,-1 do
		local tType = params_DoorTiles[iDoorTiles]
		for iTiles=#tType.tiles,1,-1 do
			local tTiles = tType.tiles[iTiles]
			local ToDelete = 1

			for _,tAdjacent in ipairs(tTiles.adjacentTiles) do
				--Keep Door Tiles adjacent to potentially spawning tiles
				for _,params_AdjacentTiles in ipairs(params.adjacentTiles) do
					if tAdjacent == params_AdjacentTiles then
						ToDelete = 0
						break
					end
				end
			end

			if ToDelete == 0 then
				for _,tAdjacent in ipairs(tTiles.adjacentTiles) do
					for _,obj in ipairs(AllObjects) do
						if tAdjacent == obj.getName() then
							ToDelete = 1
							break
						end
					end
				end
			end

			if ToDelete == 1 then
				table.remove(tType.tiles, iTiles)
			end
		end
	end




	------------------
	--Spawn Room Tiles
	------------------
	spawnRoomTiles(params_MapTiles,params_DoorTiles)




	----------------
	--Spawn Monsters
	----------------
	--Only Send maptile information for adjacentTiles
	for _,tAdjacent in ipairs(params.adjacentTiles) do
		for _,tMapTiles in ipairs(ScenarioInformation.mapTiles) do
			if tAdjacent == tMapTiles.tile then
				table.insert(params_Monsters,tMapTiles)
				break
			end
		end
	end
	spawnMonsters(params_Monsters,ScenarioInformation.gridType,params.adjacentTiles)



	----------
	--Clean Up
	----------
	function cleanUpParameters()
		wait(Delay_Delete)
		IsBusy = 0
		return 1
	end
	startLuaCoroutine(self, "cleanUpParameters")
end
